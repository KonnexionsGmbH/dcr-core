<Annotation id="ANN0" type="Link">
 <Box llx="399.31" lly="745.77" urx="475.35" ury="758.42"/>
 <Action type="URI" trigger="activate" URI="https://www.faz.net/aktuell/politik/thema/angela-merkel"/>
